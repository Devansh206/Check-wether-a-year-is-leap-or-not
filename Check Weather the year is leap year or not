#include<stiod.h>
int main()
{
 int yr;
 priintf("\n enter a year ");
 scanf("%d",&yrr);
 if (yr%100==0)
   {
     if (yr%400==0)
      printf("The year is leap year");
     else
      printf("year is not leap year");
    }
 else
    {
      if(yr%4==0)
